<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="40dp"
    tools:context=".guanyuwomen">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="20dp"



    >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="免责声明"
        android:textSize="20dp"
        android:gravity="center"
        android:textColor="#000"
        android:textStyle="bold"
        android:id="@+id/mianzetextview"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/mianzetextview"
android:id="@+id/mianzeleirong"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mianzeleirong"
        android:layout_marginTop="280dip"
        android:gravity="center"
        android:textColor="#ababab"

        android:text="版本：V1.0.0"
        />
</RelativeLayout>
</android.support.constraint.ConstraintLayout>